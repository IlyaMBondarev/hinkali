                        <div class="basket__bottom-block">
                            <p class="basket__text">Сумма заказа:</p>
                            <p class="basket__total"><span>4050</span> <span class="bold">₽</span></p>
                        </div>
                        <div class="basket__bottom-block">
                            <a href="catalog.html" class="basket__add-more slide-in">дополнить заказ</a>
                            <a href="ordering.html" class="basket__confirm">оформить заказ</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- @import "footer.html" -->
    </main>
</div>
<!-- @import "js.html" -->
</body>
</html>